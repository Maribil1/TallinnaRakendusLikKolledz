@model TallinnaRakendusLikKolledz.Models.Delinquent
@{
	ViewData["Title"] = "Edit";
}
<div class="row">
	<div class="col-md-4">
		<form asp-action="Edit">
			<div asp-validation-summary="ModelOnly" class="text-danger"></div>

			<div>
				<label asp-for="FirstName" class="control-label"></label>
				<input asp-for="FirstName" class="form-control" />
				<span asp-validation-for="FirstName" class="text-danger"></span>
			</div>
			<div>
				<label asp-for="LastName" class="control-label"></label>
				<input asp-for="LastName" class="form-control" />
				<span asp-validation-for="LastName" class="text-danger"></span>
			</div>
			<div>
				<label asp-for="Violation" class="control-label"></label>
				<input asp-for="Violation" class="form-control" />
				<span asp-validation-for="Violation" class="text-danger"></span>
			</div>
			<div>
				<label asp-for="Position" class="control-label"></label>
				<input asp-for="Position" class="form-control" />
				<span asp-validation-for="Position" class="text-danger"></span>
			</div>
			<div>
				<label asp-for="Solved" class="control-label"></label>
				<input asp-for="Solved" class="form-control" />
				<span asp-validation-for="Solved" class="text-danger"></span>
			</div>
			<div>
				<label asp-for="Solving" class="control-label"></label>
				<input asp-for="Solving" class="form-control" />
				<span asp-validation-for="Solving" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Uuenda" class="btn btn-primary" /> | <a asp-action="Index">Tühista</a>
			</div>
		</form>
	</div>
</div>